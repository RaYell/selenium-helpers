<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net40;net45;netstandard2.0</TargetFrameworks>
	<TargetFrameworks Condition="'$(OS)' != 'Windows_NT'">netstandard2.0</TargetFrameworks>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <CodeAnalysisRuleSet>Selenium.WebDriver.Extensions.ruleset</CodeAnalysisRuleSet>
    <LangVersion>latest</LangVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
	<Authors>Softlr</Authors>
    <Description>Contains Selenium WebDriver extensions including jQuery, Sizzle selector support.</Description>
  </PropertyGroup>
  <ItemGroup>
    <AdditionalFiles Include="..\..\_analysis\stylecop.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="codecracker.CSharp" Version="1.1.0">
		<PrivateAssets>all</PrivateAssets>
	</PackageReference>
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" />
    <PackageReference Include="PostSharp.Patterns.Model" Version="6.0.16-rc">
		<PrivateAssets>all</PrivateAssets>
	</PackageReference>
	<PackageReference Include="PostSharp.Patterns.Model.Redist" Version="6.0.16-rc" />
    <PackageReference Include="Selenium.Support" Version="3.13.0" />
    <PackageReference Include="SonarAnalyzer.CSharp" Version="7.2.0.5463">
		<PrivateAssets>all</PrivateAssets>
	</PackageReference>
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.0-beta008">
		<PrivateAssets>all</PrivateAssets>
	</PackageReference>
  </ItemGroup>
</Project>